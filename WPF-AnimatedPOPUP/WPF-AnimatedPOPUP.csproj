<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>WPF_AnimatedPOPUP</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Home.png" />
    <None Remove="Assets\Profile.png" />
    <None Remove="Assets\Settings.png" />
    <None Remove="Assets\TurnOff.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Home.png" />
    <Resource Include="Assets\Profile.png" />
    <Resource Include="Assets\Settings.png" />
    <Resource Include="Assets\TurnOff.png" />
  </ItemGroup>

</Project>
